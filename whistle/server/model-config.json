{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "Team": {
    "dataSource": "mongodb",
    "public": true
  },
  "Referee": {
    "dataSource": "mongodb",
    "public": true
  },
  "Director": {
    "dataSource": "mongodb",
    "public": true
  },
  "League": {
    "dataSource": "mongodb",
    "public": true
  },
  "Game": {
    "dataSource": "mongodb",
    "public": true
  },
  "AccessToken": {
    "dataSource": "mongodb",
    "public": false,
    "relations": {
      "referee": {
        "type": "belongsTo",
        "model": "Referee",
        "foreignKey": "refereeId"
      },
      "director": {
        "type": "belongsTo",
        "model": "Director",
        "foreignKey": "directorId"
      }
    }
  }
}
